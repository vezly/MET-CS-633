<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

    
 <hibernate-mapping>  
  <class name="defectTrackingSystem.model.User" table="DEFECTTRACKINGUSER">  
    <id name="UserId">  
     <generator class="sequence"><param name="sequence">user_id_seq</param>
     </generator>  
    </id>             
    <property name="firstName" column="FIRSTNAME" type="string"></property>  
    <property name="lastName" column="LASTNAME"></property>             
    <property name="userRoleId" column="USERROLEID"></property>             
  </class>  
</hibernate-mapping> 